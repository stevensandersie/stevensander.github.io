{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "@vercel/php@0.7.4"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(?:css|js|svg|png|jpg|jpeg|gif))$",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}